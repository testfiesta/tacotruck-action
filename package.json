  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/node": "^24.2.1",
    "@vercel/ncc": "^0.38.3",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1"
  }
